{
  "name": "pokedb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "xo --quiet",
    "precommit": "lint-staged"
  },
  "keywords": [
    "node"
  ],
  "files": [
    "src"
  ],
  "xo": {
    "extends": [
      "prettier"
    ],
    "rules": {
      "prefer-promise-reject-errors": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "camelcase": 0,
      "dot-notation": 0,
      "not-return-await": 0,
      "new-cap": 0,
      "unicorn/prevent-abbreviations": 0,
      "import/no-unassigned-import": 0,
      "no-global-assign": 0,
      "unicorn/no-nested-ternary": 0,
      "no-await-in-loop": 0,
      "no-negated-condition": 0,
      "no-return-await": 0,
      "no-implicit-coercion": 0,
      "no-prototype-builtins": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --semi false --single-quote --trailing-comma --write",
      "git add"
    ]
  },
  "author": "Raphael Anjos",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-polyfill": "6.26.0",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "xo": "0.30.0"
  },
  "dependencies": {
    "apollo-server-koa": "2.14.2",
    "axios": "0.21.1",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "graphql": "15.0.0",
    "graphql-tools": "5.0.0",
    "hoopa-logger": "1.1.0",
    "koa": "2.11.0",
    "nodemon": "2.0.3",
    "rethinkdb": "2.4.2",
    "rethinkly": "1.2.4"
  }
}
